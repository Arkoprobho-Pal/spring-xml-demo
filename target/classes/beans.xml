<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="actor" class="com.stackroute.domain.Actor">
    <constructor-arg name="name" value="Allu Argun"/>
    <constructor-arg name="gender" value="Male"/>
    <constructor-arg name="age" value="34"/>
    </bean>

    <bean id="actor1" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="Rajnikanth"/>
        <constructor-arg name="gender" value="Male"/>
        <constructor-arg name="age" value="65"/>
    </bean>
    <bean name="movieA" class="com.stackroute.domain.Movie" autowire="byName"></bean>
   <bean name="movieB" class="com.stackroute.domain.Movie" autowire="constructor"></bean>
    <bean id="beanlifecycledemobean" class="com.stackroute.demo.BeanLifecycleDemoBean" init-method="customInit" destroy-method="customDestroy"></bean>
    <bean id="beanpostprocessordemobean" class="com.stackroute.demo.BeanPostProcessorDemoBean"></bean>


</beans>